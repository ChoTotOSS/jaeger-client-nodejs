{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["standard", "prettier", "prettier/babel"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "semi": "off",
    "indent": ["warn", 2],
    "quotes": ["warn", "single"],
    "no-console": "warn",
    "space-before-function-paren": "off",
    "camelcase":"off"
  }
}